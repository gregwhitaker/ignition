{
   "Description":"IAM group for admin users",
   "Resources":{
      "AdminGroup":{
         "Type":"AWS::IAM::Group",
         "Properties":{
            "Path":"/",
            "Policies":[
               {
                  "PolicyName":"Administrator",
                  "PolicyDocument":{
                     "Statement":[
                        {
                           "Effect":"Allow",
                           "Action":"*",
                           "Resource":"*"
                        }
                     ]
                  }
               }
            ]
         }
      }
   }
}